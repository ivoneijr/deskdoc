<p id="notice"><%= notice %></p>

<h1>Listing Users Documents</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users_documents.each do |users_document| %>
      <tr>
        <td><%= link_to 'Show', users_document %></td>
        <td><%= link_to 'Edit', edit_users_document_path(users_document) %></td>
        <td><%= link_to 'Destroy', users_document, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Users document', new_users_document_path %>
